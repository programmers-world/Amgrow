@{
}
@model List<Microsoft.AspNetCore.Identity.IdentityUser>

    <h2>User List</h2>

    <table>
        <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
            }
        </tbody>
    </table>
